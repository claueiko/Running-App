<section class="coach-page">
    <p class="page-title">CLUB</p>
    <div class="coach-row1">
        <!-- <div class="coach-column-photo">
            <img class="profile-picture"src="/images/avatar.jpeg" alt="" width="200px">
        </div> -->
        <div class="club-column">
            <p class="name"> <%= club.nameClub %></p> <br>
            <p>Email: <%= club.emailAddressClub %></p>
            <p>Club Website: <a target="_blank" href="<%= club.linkClub %> "><%= club.linkClub %></a></p>
            <p>Region: <%= club.region.nameRegion %> </p> 
            <br><p class="moment-class">Added: <%= moment(club.createdAt).fromNow() %></p>
            <p class="moment-class">Last Updated: <%= moment(club.updatedAt).fromNow() %></p>
            <a class="edit-button" href="/club/edit?id=<%= club._id %>">Edit profile</a>
        </div>
        
    </div>

    <br><br><br>
    <p class="page-title">OUR COACHES</p>
    <div class="training-plan">

        <% club.coach.forEach(function(coach) { %>
                <a href="/coach/detail?id=<%= coach._id %>"> <p> <%= coach.nameCoach %> <span class="list-age">  </p></a>
               
            <% }) %>

    </div>
    <br><br><br>
    


</section>
<br>
<div class="event-row">
    <a target="_blank" href="<%= club.linkClub %> " class="event-link">JOIN US</a>

   </div>
<br>
